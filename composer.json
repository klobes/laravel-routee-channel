{
    "name": "klobes/laravel-routee-channel",
    "description": "Notification channel for routee.net",
    "homepage": "https://github.com/klobes/laravel-routee-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Klodjan bracja",
            "email": "klodbrace@gmail.com",
            "homepage": "https://github.com/klobes?tab=repositories"
        }
    ],
    "require": {
        "php": ">=7.1.0"
       },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "phpunit/phpunit": ">=7.0 < 10"
    },
    "autoload": {
        "psr-4": {
            "Routee\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Routee\\Test\\": "tests"
        }
    },
   "config": {
        "lock": false
    }
}
